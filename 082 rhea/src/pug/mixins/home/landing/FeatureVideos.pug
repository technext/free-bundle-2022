                  .card.h-100.py-md-2(style="min-height:113px")
                    .card-body.d-flex.flex-column.justify-content-center
                      p.mb-0.fw-semi-bold.text-500 Tanzania’s deadly pink lake
                      p.card-text
                        small.text-800 05.24
                .col-12.col-md-11.col-lg-12.col-xl-10.col-xxl-7.px-xxl-0.ps-lg-0.ps-md-4
                  .card.h-100.py-md-2(style="min-height:113px")
